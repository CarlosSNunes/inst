{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "Care-plus": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/browser",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "assets": [
                            "src/assets",
                            "src/android-chrome-36x36.png",
                            "src/android-chrome-48x48.png",
                            "src/android-chrome-72x72.png",
                            "src/android-chrome-96x96.png",
                            "src/android-chrome-144x144.png",
                            "src/android-chrome-192x192.png",
                            "src/apple-touch-icon-57x57.png",
                            "src/apple-touch-icon-60x60.png",
                            "src/apple-touch-icon-72x72.png",
                            "src/apple-touch-icon-76x76.png",
                            "src/apple-touch-icon-114x114.png",
                            "src/apple-touch-icon-120x120.png",
                            "src/apple-touch-icon-144x144.png",
                            "src/apple-touch-icon-152x152.png",
                            "src/favicon-16x16.png",
                            "src/favicon-32x32.png",
                            "src/favicon-96x96.png",
                            "src/favicon-128x128.png",
                            "src/favicon-196x196.png",
                            "src/favicon.ico",
                            "src/manifest.json",
                            "src/mstile-70x70.png",
                            "src/mstile-144x144.png",
                            "src/mstile-150x150.png",
                            "src/mstile-310x150.png",
                            "src/mstile-310x310.png",
                            "src/browserconfig.xml",
                            "src/robots.txt",
                            "src/sitemap.xml",
                            "src/manifest.webmanifest",
                            "src/web.config",
                            { "glob": "**/*", "input": "src/coronavirus", "output": "/coronavirus/" },
                            { "glob": "**/*", "input": "src/ocupacional", "output": "/ocupacional/" },
                            { "glob": "**/*", "input": "src/welcome-back", "output": "/welcome-back/" },
                            { "glob": "**/*", "input": "src/tytocare", "output": "/tytocare/" }                                    
                        ],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/videogular2/fonts/videogular.css",
                            "src/assets/css/bulma-tooltip.min.css"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/sass"
                            ]
                        },
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "homolog": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.homolog.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "homolog-windows": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.homolog-windows.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "staging": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.staging.ts"
                            }],
                            "baseHref": "/institucional/",
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "staging-cp": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.staging-cp.ts"
                            }],
                            "baseHref": "/",
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "uat-cp": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.uat-cp.ts"
                            }],
                            "baseHref": "/",
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "qa-cp": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.qa-cp.ts"
                            }],
                            "baseHref": "/",
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "es5": {
                            "tsConfig": "./tsconfig.es5.json"
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "Care-plus:build",
                        "port": 4300
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "Care-plus:build:production"
                        },
                        "homolog": {
                            "browserTarget": "Care-plus:build:homolog"
                        },
                        "homolog-windows": {
                            "browserTarget": "Care-plus:build:homolog-windows"
                        },
                        "staging": {
                            "browserTarget": "Care-plus:build:staging"
                        },
                        "staging-cp": {
                            "browserTarget": "Care-plus:build:staging-cp"
                        },
                        "qa-cp": {
                            "browserTarget": "Care-plus:build:qa-cp"
                        },
                        "uat-cp": {
                            "browserTarget": "Care-plus:build:uat-cp"
                        },
                        "es5": {
                            "browserTarget": "Care-plus:build:es5"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "Care-plus:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "codeCoverageExclude": [
                            "src/assets/**",
                            "src/models/**",
                            "src/utils/**"
                        ],
                        "codeCoverage": true,
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/assets",
                            "src/android-chrome-36x36.png",
                            "src/android-chrome-48x48.png",
                            "src/android-chrome-72x72.png",
                            "src/android-chrome-96x96.png",
                            "src/android-chrome-144x144.png",
                            "src/android-chrome-192x192.png",
                            "src/apple-touch-icon-57x57.png",
                            "src/apple-touch-icon-60x60.png",
                            "src/apple-touch-icon-72x72.png",
                            "src/apple-touch-icon-76x76.png",
                            "src/apple-touch-icon-114x114.png",
                            "src/apple-touch-icon-120x120.png",
                            "src/apple-touch-icon-144x144.png",
                            "src/apple-touch-icon-152x152.png",
                            "src/favicon-16x16.png",
                            "src/favicon-32x32.png",
                            "src/favicon-96x96.png",
                            "src/favicon-128x128.png",
                            "src/favicon-196x196.png",
                            "src/favicon.ico",
                            "src/manifest.json",
                            "src/mstile-70x70.png",
                            "src/mstile-144x144.png",
                            "src/mstile-150x150.png",
                            "src/mstile-310x150.png",
                            "src/mstile-310x310.png",
                            "src/browserconfig.xml",
                            "src/robots.txt",
                            "src/sitemap.xml",
                            "src/manifest.webmanifest"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "Care-plus:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "Care-plus:serve:production"
                        },
                        "homolog": {
                            "devServerTarget": "Care-plus:serve:homolog"
                        },
                        "homolog-windows": {
                            "devServerTarget": "Care-plus:serve:homolog-windows"
                        },
                        "staging": {
                            "devServerTarget": "Care-plus:serve:staging"
                        },
                        "staging-cp": {
                            "devServerTarget": "Care-plus:serve:staging-cp"
                        },
                        "uat-cp": {
                            "devServerTarget": "Care-plus:serve:uat-cp"
                        },
                        "qa-cp": {
                            "devServerTarget": "Care-plus:serve:qa-cp"
                        }
                    }
                },
                "server": {
                    "builder": "@angular-devkit/build-angular:server",
                    "options": {
                        "outputPath": "dist/server",
                        "main": "src/main.server.ts",
                        "tsConfig": "tsconfig.server.json"
                    },
                    "assets":[
                        { "glob": "**/*", "input": "src/coronavirus", "output": "/coronavirus/" },
                        { "glob": "**/*", "input": "src/ocupacional", "output": "/ocupacional/" },
                        { "glob": "**/*", "input": "src/welcome-back", "output": "/welcome-back/" },
                        { "glob": "**/*", "input": "src/tytocare", "output": "/tytocare/" } 

                    ],
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }],
                            "sourceMap": false,
                            "optimization": {
                                "scripts": false,
                                "styles": true
                            }
                        },
                        "homolog": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.homolog.ts"
                            }],
                            "sourceMap": false,
                            "optimization": {
                                "scripts": false,
                                "styles": true
                            }
                        },
                        "homolog-windows": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.homolog-windows.ts"
                            }],
                            "sourceMap": false,
                            "optimization": {
                                "scripts": false,
                                "styles": true
                            }
                        },
                        "staging": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.staging.ts"
                            }],
                            "sourceMap": false,
                            "optimization": {
                                "scripts": false,
                                "styles": true
                            }
                        },
                        "staging-cp": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.staging-cp.ts"
                            }],
                            "sourceMap": false,
                            "optimization": {
                                "scripts": false,
                                "styles": true
                            }
                        },
                        "uat-cp": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.uat-cp.ts"
                            }],
                            "sourceMap": false,
                            "optimization": {
                                "scripts": false,
                                "styles": true
                            }
                        },
                        "qa-cp": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.qa-cp.ts"
                            }],
                            "sourceMap": false,
                            "optimization": {
                                "scripts": false,
                                "styles": true
                            }
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "Care-plus"
}